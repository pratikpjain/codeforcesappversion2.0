        app:layout_constraintVertical_bias="0.907" />

    <TextView
        android:id="@+id/lname"
        android:layout_width="182dp"
        android:layout_height="52dp"
        android:hint="N/A"
        android:textColor="#1B0202"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.877"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.83" />

    <Button
        android:id="@+id/button2"
        android:layout_width="239dp"
        android:layout_height="48dp"
        android:text="View Submissions"
        android:onClick="mybtn"
        app:backgroundTint="#958F8F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.419"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.264"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.108" />

    <TextView
        android:id="@+id/ls"
        android:layout_width="199dp"
        android:layout_height="33dp"
        android:fontFamily="@font/antic"
        android:textColor="#651FFF"
        app:layout_constraintBottom_toTopOf="@+id/hdl"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.641"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titlephoto"
        app:layout_constraintVertical_bias="0.568" />

    <Button
        android:id="@+id/addto"
        android:layout_width="91dp"
        android:layout_height="50dp"
        android:text="Friend"
        android:textSize="8sp"
        app:backgroundTint="#6E86E1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.938"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.072" />

    <Button
        android:id="@+id/remove"
        android:layout_width="90dp"
        android:layout_height="46dp"
        android:text="UnFriend"
        android:textSize="8sp"
        app:backgroundTint="#6E86E1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.941"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.152" />

